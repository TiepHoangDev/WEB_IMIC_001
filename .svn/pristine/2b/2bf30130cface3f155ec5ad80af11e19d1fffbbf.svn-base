p {
 font-family:"Helvetica Neue",Helvetica,Arial,sans-serif
}
.nav#menu .open>a,.nav#menu .open>a:focus,.nav#menu .open>a:hover {
 background-color:transparent
}
.mega-dropdown {
 position:static!important
}
.mega-dropdown-menu:after,.mega-dropdown-menu:before {
 content:"";
 position:absolute
}
.dropdown>a>.fa-angle-down,.mega-dropdown>a>.fa-angle-down {
 -webkit-transition:all .2s linear;
 -moz-transition:all .2s linear;
 -ms-transition:all .2s linear;
 -o-transition:all .2s linear;
 transition:all .2s linear
}
.dropdown:hover>a>.fa-angle-down,.mega-dropdown:hover>a>.fa-angle-down {
 -moz-transform:rotate(-180deg);
 -webkit-transform:rotate(-180deg);
 -o-transform:rotate(-180deg);
 -ms-transform:rotate(-180deg);
 transform:rotate(-180deg)
}
.mega-dropdown-menu {
 padding:20px 0 0;
 width:100%;
 box-shadow:none;
 -webkit-box-shadow:none;
 background-color:#333b3f;
 margin:0;
 border:none;
 border-left:1px solid #333b3f;
 border-right:1px solid #333b3f;
 border-radius:0;
 border-top:1px solid #2e3539
}
.mega-dropdown-menu:before {
 border-bottom:9px solid #333b3f;
 border-right:9px solid transparent;
 border-left:9px solid transparent;
 top:-9px;
 left:300px;
 z-index:10
}
.mega-dropdown-menu:after {
 border-bottom:10px solid #2b3032;
 border-right:10px solid transparent;
 border-left:10px solid transparent;
 top:-10px;
 left:299px;
 z-index:8
}
.mega-dropdown-menu .margin-auto-megamenu {
 margin:0 auto
}
.mega-dropdown-menu>li>ul {
 padding:0;
 margin:0
}
.mega-dropdown-menu>li>ul>li {
 list-style:none
}
.mega-dropdown-menu>li>ul>li>a {
 display:block;
 padding:3px 20px;
 clear:both;
 font-weight:400;
 line-height:1.428571429;
 color:#999;
 white-space:normal
}
ul#menu ul ul li.col-mega a:hover {
 text-decoration:none!important
}
.mega-dropdown-menu>li ul>li>a:focus,.mega-dropdown-menu>li ul>li>a:hover {
 text-decoration:none;
 color:#444;
 background-color:#333b3f
}
ul#menu li.dropdown:first-child a.dropdown-toggle:hover {
 color:#0092ff;
 text-decoration:none
}
.mega-dropdown-menu .dropdown-header {
 color:#428bca;
 font-size:18px;
 font-weight:700
}
.col-mega {
 text-align:center;
 color:#fff;
 padding:30px 0;
 cursor:pointer
}
.col-mega img {
 height:100px;
 ul#menulia:hover;
 display:inline-block;
 margin:0 auto 15px
}
.col-mega h3 {
 color:#c8c8c7;
 font-family:brandon_textbold;
 text-transform:uppercase
}
.col-mega p {
 color:#fff;
 margin-bottom:20px;
 padding:0 10px;
 font-family:"Helvetica Neue",Helvetica,Arial,sans-serif
}
.border-top-mega {
 border-top:1px solid #2a2f32
}
.border-top-mega p {
 color:#898989;
 padding:15px 0;
 margin:0
}
ul#menu .mega-dropdown-menu .margin-auto-megamenu .btn-more-mega {
 text-transform:uppercase;
 font-size:16px;
 background-color:#1666a2;
 color:#fff;
 text-align:center;
 display:inline-block;
 padding:8px 29px
}
ul#menu .mega-dropdown-menu .margin-auto-megamenu .btn-more-mega:hover {
 background-color:#228ede;
 text-decoration:none;
 border-radius:4px
}
.dropdown.mega-dropdown .fa.fa-angle-down {
 font-size:22px;
 line-height:0;
 margin-top:11px;
 margin-left:5px;
 vertical-align:top
}
.navbar.navbarmain {
 border:none
}
.partner-megamenu {
 padding:20px 0
}
.partner-megamenu p {
 color:#898989;
 font-size:14px;
 margin-bottom:0;
 text-align:right
}
.row-megapartner .tt {
 width:32%;
 display:inline-block
}
.row-megapartner img {
 display:inline-block;
 width:67%;
 vertical-align:baseline;
 padding-left:15px
}
.bd-hrw nav.navbar.hv,.bodychannel nav.navbar.hv,.homepage nav.navbar.hv {
 background-color:#30373b!important;
 transition-property:background-color;
 transition-duration:1s;
 transition-timing-function:linear
}
ul#menu ul ul.margin-auto-megamenu li.col-mega a {
 padding-bottom:0;
 padding-top:0
}
.col-mega p span {
 display:block
}
@media(min-width:1200px) {
 .container.margin-auto-megamenu {
  width:1000px
 }
 .col-mega p {
  font-size:13px;
  line-height:180%;
  padding:10px 0 0;
  min-height:52px
 }
 .col-mega h3 {
  font-size:16px;
  padding-bottom:15px
 }
}
@media(min-width:768px) and (max-width:1199px) {
 .dropdown-menu.megamenu {
  top:45px
 }
 .col-mega p {
  font-size:13px;
  line-height:180%;
  padding:10px 0 0;
  min-height:52px
 }
 .col-mega h3 {
  font-size:16px;
  padding-bottom:15px
 }
}
@media(max-width:1024px) {
 .mega-dropdown-menu {
  top:86%
 }
}
