    content: "\f04b";
}

.jp-controls-container .jp-controls .jp-pause:before {
    content: "\f04c";
}

.jp-controls-container .jp-controls .jp-mute:before {
    content: "\f028";
}

.jp-controls-container .jp-controls .jp-pause:before {
    content: "\f04c";
}

.jp-controls-container .jp-controls .jp-unmute:before {
    content: "\f026";
}

.jp-controls-container .jp-controls .jp-mute,
