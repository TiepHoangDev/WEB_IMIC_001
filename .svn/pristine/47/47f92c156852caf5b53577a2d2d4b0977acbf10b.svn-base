
body {
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
    line-height: 1.7;
    font-weight: lighter;
    color: #374456;
    background: #fff;
}

.site {
    position: relative;
    overflow-x: hidden;
}
.logo { font-size:22px; font-weight:700;}
.logo span {color:#fe6f10;}
.text-tuvan {
	}
.text-tuvan h2 {
	color:#fff;
	text-align:center;
	font-size:22px;
	    font-family: 'Roboto', sans-serif;
	}
.text-tuvan p {
	color:#fff;
	text-align:center;
	font-size:16px;
	}
.text-tuvan .border {
	background-color:#02cdb3;
	width:60px;
	height:4px;
	margin:auto;
	}
.post-wrapper {
    position: relative;
    overflow: hidden;
    min-height: 180px;
	padding: 40px 0 0;
}	
.post-wrapper .post-thumb {
    position: relative;
}
 .post-info {
    background: rgba(0,0,0,0.5);
    text-align: left;
    position: absolute;
    padding: 20px 10px 20px 30px;
    left: 0;
    right: 0;
    bottom: 0;
    -webkit-transition: all 0.4s ease-out;
    -moz-transition: all 0.4s ease-out;
    -ms-transition: all 0.4s ease-out;
    -o-transition: all 0.4s ease-out;
}
.post-wrapper .post-info {
    position: static;
    background: none;
    padding: 20px 0;
    min-height: 180px;
}
.post-info h3.post-title {
    font-size: 12px;
    text-transform: uppercase;
    margin-bottom: 0;
}
.post-wrapper .post-info .post-excerpt {
    height: 100%;
    -webkit-opacity: 1;
    -moz-opacity: 1;
    opacity: 1;
    -webkit-transform: scaleY(1);
    -moz-transform: scaleY(1);
    -ms-transform: scaleYscaleY(1);
    -o-transform: scaleY(1);
    line-height: 20px;
    margin: 20px 0;
}
.post-info .post-excerpt p {
    color: #d4d4d4;
    margin: 0;
    font-style: italic;
}
.post-info.congdong .post-excerpt p{ font-style:normal;}
.post-thumb a {
    display: block;
}
.post-thumb img {
    width: 100%;
	max-width:100%;
}
.post-wrapper .post-thumb .post-c{
	width:100%;
	position:absolute;
	height:35px;
	right:0px;
	bottom:0px;
	z-index:2;
	}
.post-wrapper .post-thumb .post-c .box {
	float:right;
	width:70px;
	height:35px;
	line-height:35px;
	font-size:12px;
	color:#fff;
	text-align:center;
	}
.post-wrapper .post-thumb .post-c .box.time-color { background-color:#46a3da;}
.post-wrapper .post-thumb .post-c .box.comment-color { background-color:#8ccef5;}
.post-wrapper .post-thumb .post-c .fa-commenting-o { margin-right:3px;}		
 .post-wrapper .post-info .post-excerpt p {
    color: #838383;
}	
.post-info .post-bottom {
    position: absolute;
    top: 0;
    width: 100%;
    height: 40px;
    line-height: 40px;
}
.post-info .post-bottom .post-date {
    padding: 0 10px;
    background-color: #18afd3;
    color: #fff;
}
.post-info .post-bottom .post-date {
    padding: 0 10px;
    background-color: #02bbff;
    color: #fff;
}
.post-info .post-bottom .post-date .fa {
    color: #fff;
}
.post-info .post-bottom .post-date span { color:#fff;}

.post-info .post-date, .post-info .comment, .post-info .author, .post-info .comment a {
     color: #838383;
    display: inline-block;
    font-size: 13px;
    margin-right: 15px;
    text-transform: capitalize;
}
.post-info .post-bottom span {
    color: #838383;
}				
