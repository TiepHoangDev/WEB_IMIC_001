@model IEnumerable<WCF.BusinessObjectsLayer.EntityObjects.ExperiencerObject>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <div class="col-xs-6 col-lg-3 col-md-4 col-ms-4">
        <div class="box-about-3">
            <img src="images/gioi-thieu/@item.ExperiencerImage" class="img">
            <div class="text-ct">
                <div class="box">
                    <h3 class="title">@item.FullName</h3>
                    <div class="text2">
                        <i class="fa fa-phone"></i> @item.Phone
                    </div>
                </div>
            </div>
        </div>
    </div>

}
