@section Scripts{
   <script src="~/Content/assets/js/jquery.simplePagination.js"></script>

    <script>
        var query = null;
        var tag = null;
    </script>
    @if (ViewBag.query != null)
    {
        <script>

            query = '@ViewBag.query';
        </script>
    }
    @if (ViewBag.tag != null)
    {

        <script>

            tag = '@ViewBag.tag';
        </script>
    }
    <script>
        $(document)
            .ready(function() {

            });

        $(function() {


            var items = '@ViewBag.total';
            var currentPage = '@ViewBag.start';
            var href = "";

            if (query != null) {
                debugger;
                href = 'tim-kiem-kien-thuc.html?query=' + query + '&start='
            } else {
                debugger;
                href = 'tim-kiem-kien-thuc.html?tag=' + tag + '&start='
            }
            $('#pagination')
                .pagination({
                    items: items,
                    itemsOnPage: 10,
                    currentPage: currentPage,
                    hrefTextPrefix: href,
                    cssStyle: 'pagination'
                });
        });

        @*function Search() {
            debugger;
            var query = $('input[name=query]').val();
            $(location).href = '@Url.Action("Search", "LessonPage")' + '?query=' + query;
        }*@

        $('.tag')
            .click(function(e) {
                var tag = $(this).data('tag');
                window.location.href = '@Url.Action("Search", "LessonPage")' + '?tag=' + tag;

            });
       

        $('.header__search__button')
      .on('click',
          function (e) {
              e.preventDefault();
              debugger;
              var query = $('input[name=query]').val();
              window.location.href = '@Url.Action("Search", "LessonPage")' + '?query=' + query;
          });
    </script>
}