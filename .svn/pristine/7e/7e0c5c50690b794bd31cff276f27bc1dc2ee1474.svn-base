@using WCF.Web.App_Start
@model IEnumerable<WCF.BusinessObjectsLayer.EntityObjects.LessonDetailObject>
@{
    Layout = "~/Views/Shared/_LayoutLesson.cshtml";
    ViewBag.Title = "Trang tìm kiếm";
    ViewBag.header = "search";
}
@section LevelCss{
    <link rel="stylesheet" href="~/Content/assets/css/content-tag.css"/>
    @*<link rel="stylesheet" type="text/css" href="~/Areas/AdminCP/Content/assets/admin/pages/css/simplePagination.css"/>*@
}
    <style>
        .activity-item {
            border: none;
            border-bottom: 1px solid #e4e4e4;
        }
        /*<!-- .activity-item:nth-child(1) { -->
                <!-- border-top: 1px solid #e4e4e4; -->
                <!--
            }
        -->*/
        .activity-item__content {
            padding-left: 0;
        }

        a:hover {
            text-decoration: none;
        }

        .comment-count {
            float: left;
            list-style: none;
            color: rgba(0, 0, 0, .54);
        }

        .comment-count li {
            display: inline-block;
            margin-right: 10px;
        }

        .activity-item__title a:hover {
            text-decoration: none;
            color: #5488c7 !important;
        }

        .activity-item__title a {
            color: black !important;
        }

        .activity-item__title {
            text-transform: uppercase;
            font-weight: bold;
        }

        .list-unstyled {
            float: right;
        }

        .list-unstyled li {
            display: inline-block;
            margin-left: 8px;
        }

        .adv {
            margin-top: 10px;
        }
    </style>




<main class="content">
<div class="left-box" style="Width: 75%; float: left; padding: 10px;">
<div style="padding: 0px 10px" class="s-large-below s-large-above">
<aside class="activity-wrap animated slideDown headroom--top">
@foreach (var item in Model)
{



    <article class="activity-item activity-item--forum" style="display: block;">
        <div class="u-clearfix u-marginBottom10">
            <div class="postMetaInline u-floatLeft">
                <div class="u-flexCenter">
                    <div class="postMetaInline-avatar u-flex0">
                        <a class="link avatar u-baseColor--link" href="">
                            <img src="~/Content/images/avatar/@item.objAccount.ImageAvatar" class="avatar-image u-size36x36 u-xs-size32x32" >
                        </a>
                    </div>
                    <div class="postMetaInline postMetaInline-authorLockup u-flex1 u-noWrapWithEllipsis">
                        <a class="link link link--darken link--accent u-accentColor--textNormal u-accentColor--textDarken u-baseColor--link" >@item.objAccount.FullName</a>
                        <div class="u-fontSize12 u-baseColor--textNormal u-textColorNormal js-postMetaInlineSupplemental"><a class="link link--darken" ><time >@item.ModifiedTime</time></a><span class="middotDivider u-fontSize12"></span><span class="readingTime"></span></div>
                    </div>
                </div>
            </div>
        </div>

        <div class="activity-item__content">
            <h3 class="activity-item__title"> <a href="@Url.RouteUrl("lesson-detail", new {code = item.DetailCode, name = StringHelpers.GenerateURL(item.DetailTitle)})" > @item.DetailTitle</a>
            </h3>

            <p style="font-size: 14px; clear: both;"> @item.DetailSummary</p>

            <ul class="comment-count">
                <li> <i class="fa fa-heart-o" aria-hidden="true"></i> @item.TotalOfLike
                <li>
                <li> <i class="fa fa-comment-o" aria-hidden="true"></i> @item.TotalOfComment
                <li>
            </ul>

            <ul class="tags list-unstyled">
                @{Html.RenderAction("Detail_SearchTag", "LessonPage", new {LessonDetailID = item.LessonDetailId});}
            </ul>
</div>

    </article>
}

