
@model IEnumerable<WCF.BusinessObjectsLayer.EntityObjects.TechRightObject>
    @foreach (var item in Model)
    {
         <div class="box-list">
        <h2 class="title"><a href="@item.TechRightUrl">@item.TechRightTitle</a></h2>
        <a href="@item.TechRightUrl">
            <img src="~/Content/Galleries/Tech/TechRight/@item.TechRightImage" class="img">
        </a>
             <ul class="newlist-2">
                 @if (!string.IsNullOrEmpty(item.TechRightLine1))
                 {
                     <li><a href="@item.TechRightUrl1">@item.TechRightLine1</a></li>
                 }
                 @if (!string.IsNullOrEmpty(item.TechRightLine2))
                {
                    <li><a href="@item.TechRightUrl2">@item.TechRightLine2</a></li>
                }

                @if (!string.IsNullOrEmpty(item.TechRightLine3))
                {
                    <li><a href="@item.TechRightUrl3">@item.TechRightLine3</a></li>
                }

                @if (!string.IsNullOrEmpty(item.TechRightLine4))
                {
                    <li><a href="@item.TechRightUrl4">@item.TechRightLine4</a></li>
                }

                @if (!string.IsNullOrEmpty(item.TechRightLine5))
                {
                    <li><a href="@item.TechRightUrl5">@item.TechRightLine5</a></li>
                }


             </ul>
    </div>
    }
   
