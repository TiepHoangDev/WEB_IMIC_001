body {
    font-family: Roboto-Regular;
    font-size: 14px;
    height: 100%;
    background-color: #fff;
    color: #575757;
}
.box_text_q h2 {
    display: inline-block;
    width: 100%;
    text-align: center;
    font-size: 28px;
    color: #333;
    margin-top: 50px;
    /* margin-bottom: 30px; */
    font-weight: 600;
}
.box_text_q h2 span {
    color: #ca3433;
}
.box_text_q {
    max-width: 1024px;
    margin: auto;
    min-height: 300px;
    /* margin-bottom: 30px; */
    width: 100%;
}
.box_text_q .text-c {
    display: inline-block;
    width: 100%;
    text-align: center;
    font-size: 18px;
    color: #333;
    margin-top: 30px;
    line-height: 30px;
    font-family: "Times New Roman", Times, serif;
}
.box-about-3 {
    width: 100%;
    display: inline-block;
    margin-top: 15px;
    margin-bottom: 15px;
    height: 100%;
    position: relative;
    overflow: hidden;
}
.box-about-3 .img {
    height: 100%;
    width: 100%;
}
.box-about-3 .text-ct {
    position: absolute;
    z-index: 2;
    left: 0px;
    top: 0px;
    bottom: 0px;
    right: 0px;
    width: 100%;
    height: 100%;
    background-color: rgba(77, 178, 230, 0.7);
    opacity: 0;
    -webkit-transition: all 1s ease-in-out;
    -moz-transition: all 1s ease-in-out;
    -ms-transition: all 1s ease-in-out;
    -o-transition: all 1s ease-in-out;
    transition: all 1s ease-in-out;
}
.box-about-3 .text-ct .box .title {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: inline-block;
    width: 100%;
    font-size: 12px;
    color: #fff;
    text-align: left;
    line-height: 40px;
    border-bottom: 1px solid #fff;
    text-transform: uppercase;
    margin: 0px;
}
.box-about-3 .text-ct .box .text2 {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: inline-block;
    width: 100%;
    font-size: 16px;
    color: #fff;
    text-align: left;
    line-height: 30px;
    text-transform: uppercase;
    margin: 0px;
}
.box-about-3 .text-ct .box {
    width: 100%;
    padding: 15px;
    position: absolute;
    left: 0px;
    bottom: 0px;
}
.box-about-3 .text-ct .box .text2 i {
    font-size: 18px;
    margin-right: 10px;
    border: 1px solid #fff;
    display: inline-block;
    height: 30px;
    width: 30px;
    border-radius: 50%;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    line-height: 30px;
    text-align: center;
}
.bg_white {
    background-color: #fff;
}

