@charset "utf-8";
/* CSS Document */
.w-100{
	width:100%;
}
.no-padding{
	padding-left:0px !important;
	padding-right:0px !important;
}
.tr-banner{
	width:100%;
	float:left;
	background-size:cover;
	background-position:center;
	padding:8% 12%;
	margin-bottom:25px;
	margin-top:-15px;
}
.tr-banner-title{
	font-size:46px;
	text-align:center;
	display:table;
	margin:auto;
	color:#FFF;
	font-weight:400;
}
.tr-banner-link{
	font-size:25px;
	text-align:center;
	display:table;
	margin:auto;
	color:#FFF;
}
.tr-banner li{
	list-style-type:none;
}
ul.tr-banner-detail{
	margin:0px;
	padding:0px;
	width:100%;
	float:left;
	margin-top:10%;
	padding-bottom:30px;
}
ul.tr-banner-detail li.li-detail{
	color:#FFF;
	list-style-type:none;
	text-align:center;
	margin-top:15px;
	margin-bottom:15px;
}
.tr-more{
	display:table;
	margin:auto;
	width:155px;
	height:40px;
	background-color:#0060ac;
	padding:0px 5px;
	line-height:40px;
	color:#FFF;
	font-size:17px;
	margin-top:20px;
	
}
.tr-banner a:hover{
	color:#FFF;
	text-decoration:underline;
}
.tr-banner i{
	margin-left:5px;
}
.tr-detail-caption{
	color:#FFF;
	font-weight:bold;
	margin:auto;
	margin-top:5px;
	text-align:center;
	font-size:20px;
}
.tr-detail-content{
	color:#FFF;
	margin:auto;
	margin-top:15px;
	text-align:center;
	font-size:18px;
	font-family:Segoe UI Light;
	
}
ul.train-list li{
	padding-bottom:7px;
	padding-top:7px;
	border-bottom:1px solid #FFF;
}
ul.train-list{
	padding-bottom:20px;
}
ul.train-list li p{
	margin:0px;
}
ul.train-list li p:first-child{
	float:left;
}
ul.train-list li p:last-child{
	float:right;
}

