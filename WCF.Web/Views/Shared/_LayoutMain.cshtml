    height: 66px;
    color: #fff;
    line-height: 20px;
    padding: 10px 0;
    z-index: 99999;
    opacity: 0.9;
}
    .nav-bottom label {
    font-weight: normal;
    font-size: 18px;
    font-family: 'Roboto Condensed',sans-serif;
}

label {
    display: inline-block;
    max-width: 100%;
    margin-bottom: 5px;
    font-weight: 700;
}
.nav-bottom p {
    font-size: 20px;
    font-weight: bold;
